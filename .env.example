# GamerStake Configuration
GAMERSTAKE_API_KEY=your-api-key-from-admin-panel
ENVIRONMENT=development  # or 'production' for live games
DEBUG=true

# Server Configuration
PORT=3000
NODE_ENV=development

# CORS: frontend origin(s). Comma-separated for multiple (e.g. Vercel + preview).
# Production (frontend on Vercel): use your Vercel app URL, no trailing slash
CORS_ORIGIN=*
# Example: CORS_ORIGIN=https://your-app.vercel.app,https://your-app-git-main-you.vercel.app
